{
  "name": "vibe-booking-platform",
  "version": "2.0.0",
  "description": "Vibe Hotels - Modern hotel booking platform with AI-powered search and luxury design",
  "type": "module",
  "packageManager": "npm@9.0.0",
  "scripts": {
    "dev": "vite --port 3009",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "analyze": "npx vite-bundle-analyzer",
    "start:payment": "node simple-payment-server.cjs",
    "turbo:build": "turbo run build",
    "turbo:test": "turbo run test:run",
    "turbo:test:e2e": "turbo run test:e2e",
    "turbo:lint": "turbo run lint",
    "turbo:typecheck": "turbo run typecheck",
    "turbo:quality": "turbo run lint typecheck test:run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.8.4",
    "@types/node": "^24.4.0",
    "@types/react-window": "^1.8.8",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.63.0",
    "react-router-dom": "^6.8.1",
    "react-window": "^2.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest-axe": "^9.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "turbo": "^2.5.8",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
