{
  "name": "vibe-booking",
  "version": "2.0.0",
  "description": "Vibe Booking - Modern hotel booking platform with AI-powered search and Square payment integration.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "setup": "./scripts/setup.sh",
    "health": "./scripts/health-check.sh",
    "pre-release": "./scripts/pre-release-check.sh",
    "clean": "rm -rf dist coverage node_modules",
    "clean:install": "npm run clean && npm install",
    "analyze": "vite-bundle-visualizer",
    "bundle:analyze": "node scripts/bundle-analyzer.js",
    "security:scan": "node scripts/security-scan.js",
    "lighthouse:ci": "lhci autorun",
    "docker:build": "docker build -t vibe-hotels:latest .",
    "docker:run": "docker run -p 3000:3000 vibe-hotels:latest .",
    "build:prod": "npm run check:all && npm run build && npm run bundle:analyze",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "release": "semantic-release",
    "check:all": "npm run lint && npm run typecheck && npm test -- --run && npm run security:scan",
    "deploy:prep": "npm run clean && npm install && npm run build:prod"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@tanstack/react-query": "^5.84.2",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "helmet": "^7.1.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-intersection-observer": "^9.5.3",
    "react-router-dom": "^6.20.1",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.1.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.54.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^2.1.0",
    "husky": "^8.0.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "semantic-release": "^22.0.12",
    "snyk": "^1.1266.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-bundle-visualizer": "^1.0.1",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,mjs,cjs}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  }
}
