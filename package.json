{
  "name": "vibe-booking",
  "version": "2.0.1",
  "description": "Vibe Booking - Luxury hotel booking platform with professional design system, AI-powered search, and Square payment integration. Features 2025 hospitality-grade UI matching Marriott/Hilton standards. Ready for deployment!",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit --skipLibCheck && vite build",
    "build:skip-errors": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\" --project=\"iPhone SE\"",
    "test:e2e:desktop": "playwright test --project=\"chromium\" --project=\"firefox\" --project=\"webkit\" --project=\"Desktop Large\"",
    "test:e2e:tablet": "playwright test --project=\"iPad\" --project=\"iPad Mini\" --project=\"Galaxy Tab\"",
    "test:e2e:conversion": "playwright test tests/e2e/conversion-elements.spec.ts tests/e2e/booking-flow.spec.ts",
    "test:e2e:responsive": "playwright test tests/e2e/responsive-design.spec.ts",
    "test:e2e:hero": "playwright test tests/e2e/hero-section.spec.ts",
    "test:e2e:search": "playwright test tests/e2e/search-results.spec.ts",
    "test:e2e:testimonials": "playwright test tests/e2e/testimonials.spec.ts",
    "test:e2e:live": "PLAYWRIGHT_BASE_URL=https://vibe-booking-platform.netlify.app playwright test tests/e2e/live-hotel-data.spec.ts",
    "test:e2e:live-all": "PLAYWRIGHT_BASE_URL=https://vibe-booking-platform.netlify.app playwright test",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:visual:update": "playwright test tests/e2e/visual-regression.spec.ts --update-snapshots",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "clean": "rm -rf dist coverage node_modules",
    "clean:install": "npm run clean && npm install",
    "analyze": "vite-bundle-visualizer",
    "start": "vite"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.84.2",
    "@types/react-window": "^1.8.8",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "helmet": "^7.1.0",
    "lucide-react": "^0.294.0",
    "puppeteer": "^24.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-intersection-observer": "^9.5.3",
    "react-router-dom": "^6.20.1",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.1.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,mjs,cjs}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  }
}
