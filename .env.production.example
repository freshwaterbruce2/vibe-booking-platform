# VIBE HOTELS - Production Environment Configuration

# Square Payment Configuration (Production)  
VITE_SQUARE_APPLICATION_ID="sq0idb-YOUR_PRODUCTION_APP_ID_HERE"
VITE_SQUARE_LOCATION_ID="YOUR_PRODUCTION_LOCATION_ID_HERE"

# Payment Settings
VITE_ENABLE_MOCK_PAYMENTS="false"

# API Configuration
VITE_API_URL="https://vibe-booking-platform.netlify.app/.netlify/functions"

# Hotel Search API
VITE_LITEAPI_KEY="YOUR_LITEAPI_KEY_HERE"

# AI Integration  
VITE_OPENAI_API_KEY="YOUR_OPENAI_KEY_HERE"

# Authentication
VITE_ENABLE_SOCIAL_LOGIN="true"
VITE_GOOGLE_CLIENT_ID="YOUR_GOOGLE_CLIENT_ID_HERE"

# Feature Flags
VITE_ENABLE_ANALYTICS="true"
VITE_ENABLE_PUSH_NOTIFICATIONS="true"
VITE_ENABLE_USER_PROFILES="true"
VITE_ENABLE_PERSONALIZATION="true"

# PWA Configuration
VITE_PWA_ENABLED="true"

# Square Web SDK Configuration
VITE_SQUARE_WEB_SDK_URL="https://js.squareup.com/v2/paymentform"
