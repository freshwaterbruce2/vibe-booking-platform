{
  "name": "build-website-example",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest tests/server.test.js tests/unit-api.test.js",
    "test:integration": "jest tests/integration.test.js",
    "test:security": "jest tests/security.test.js",
    "test:performance": "jest tests/performance.test.js",
    "test:e2e": "playwright test tests/e2e.test.js",
    "test:accessibility": "playwright test tests/accessibility.test.js",
    "test:pwa": "playwright test tests/pwa.test.js",
    "test:browser-compat": "playwright test tests/browser-compatibility.test.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:performance",
    "test:all-e2e": "npm run test:e2e && npm run test:accessibility && npm run test:pwa && npm run test:browser-compat",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:production": "NODE_ENV=production npm run test:all",
    "start": "nodemon server/server.js",
    "start:prod": "NODE_ENV=production node server/server.js",
    "start:test": "NODE_ENV=test PORT=3001 node server/server.js",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss client/styles.css -o client/dist/styles.min.css",
    "build:js": "terser client/app.js client/passion-data.js client/passion-ui.js client/payment-utils.js -o client/dist/app.min.js --source-map",
    "optimize:images": "imagemin client/images/* --out-dir=client/dist/images --plugin=imagemin-mozjpeg --plugin=imagemin-pngquant",
    "lint": "eslint server/ client/ --ext .js",
    "lint:fix": "eslint server/ client/ --ext .js --fix",
    "docker:build": "docker build -t hotel-booking .",
    "docker:run": "docker run -p 3001:3001 --env-file .env hotel-booking",
    "validate:production": "npm run lint && npm run test:all && npm run test:all-e2e"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@square/web-payments-sdk-types": "^1.77.0",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "liteapi-node-sdk": "^3.0.4",
    "node-cache": "^5.1.2",
    "squareup": "^1.0.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@playwright/test": "^1.40.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.1.4",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "supertest": "^6.3.3",
    "terser": "^5.24.0"
  }
}
