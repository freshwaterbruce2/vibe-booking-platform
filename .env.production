# Vibe Booking Platform - Production Environment
# Simple deployment configuration for Netlify + Railway

# Frontend (Netlify) - Will be set in build environment
VITE_API_URL=https://vibe-booking-backend-production.up.railway.app
VITE_OPENAI_API_KEY=your_openai_key_here
VITE_LITEAPI_KEY=your_liteapi_key_here
SQUARE_APPLICATION_ID=your_square_app_id_here
SQUARE_LOCATION_ID=your_square_location_id_here

# Backend (Railway) - Will be set in Railway environment
NODE_ENV=production
PORT=3001
DATABASE_URL=postgresql://user:password@host:5432/vibehotels
SQUARE_ACCESS_TOKEN=your_square_access_token_here
SQUARE_ENVIRONMENT=production
JWT_SECRET=your_jwt_secret_here
COMMISSION_RATE=0.05